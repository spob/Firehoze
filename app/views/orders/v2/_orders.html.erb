<% pane = params[:pane] if params[:pane] %>
<% orders = @orders if pane == "orders" %>

<div id="<%= pane %>">
  <% if orders.present? %>
      <table>
        <tr>
          <th>Order Number</th>
          <th>Order Date</th>
          <th>Amount</th>
        </tr>
      </table>
  <% else %>
      No lessons
  <% end %>
</div>
