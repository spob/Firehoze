<h1>Edit User</h1>
            
<% form_for(:user, :url => user_path(@user), :html => { :method => :put }) do |f| %>
    <%= f.error_messages %>
    <%= render :partial => "form", :object => f %>
    <%= f.submit "Update" %>
    <%= link_to "Cancel", user_path(@user) %>
<% end %>

<br/><%= link_to "My Profile", account_path %>
