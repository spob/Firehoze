<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>
		<general ui_visibility_time="6" />
		<auto_play>
			<defaults symbol="circular" time="6"/>
			<tweenIn x="890" y="30" width="35" height="35" tint="0xFFFFFF"/>
		</auto_play>

		<prev_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenIn scaleX="1.2" /> 
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000"/>
		</prev_button>

		<prev_symbol>
			<tweenOver tint="0x000000"/>
		</prev_symbol>

		<next_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000"/>
			<tweenIn scaleX="1.2" /> 
		</next_button>

		<next_symbol>
			<tweenOver tint="0x000000"/>
		</next_symbol>

		<description>
			<defaults round_corners="0, 0, 0, 0" 
				heading_text_size="42" 
				heading_text_color="0xffffff" 
				heading_text_margin="0, 0, 0, 10"  
				paragraph_text_size="18" 
				paragraph_text_margin="0, 0, 20, 20" 
				paragraph_text_color="0xFFFFFF" />
				<tweenIn x="0" y="170" width="950" height="80" alpha="0.70"/>
				<tweenOver alpha="0.80"/>
			</description>
		</settings>

		<slides> 

			<!-- transition 1-to-2  -->
			<transition num="1" slicing="horizontal" direction="right" duration="0.8" delay="0.05" z_multiplier="5"/>

			<!-- slide 1 -->
			<slide>
				<url>images/cuber/vlaunch/4-bridges.jpg</url>
				<description>
					<heading>Bridging</heading>
					<paragraph>customers with entrepreneurs ...</paragraph>
				</description>
			</slide>		

			<!-- transition 2-to-3  -->
			<transition num="2" direction="left" shader="flat" />

			<!-- slide 2 -->
			<slide>
				<url>images/cuber/vlaunch/1-pen-and-paper.jpg</url>
				<description>
					<heading>Unlock</heading>
					<paragraph>your startup's potential  ...</paragraph>
				</description>
			</slide>		

			<!-- transition 3-to-4  -->
			<transition num="3" direction="left" shader="flat" />

			<!-- slide 3-->
			<slide>
				<url>images/cuber/vlaunch/2-glasses-and-notebook.jpg</url>
				<description>
					<heading>Focus</heading>
					<paragraph>on how to get a customer ...</paragraph>
				</description>
			</slide>

			<!-- transition 4-to-5  -->
			<transition num="4" direction="right" shader="flat" />

			<!-- slide 4 -->
			<slide>
				<url>images/cuber/vlaunch/3-test-tubes.jpg</url>
				<description>
					<heading>Build</heading>
					<paragraph>technology that matters ...</paragraph>
				</description>
			</slide>

			<!-- transition 5-to-6  -->
			<transition num="5" direction="right" shader="flat" />

			<!-- slide 5 -->
			<slide>
				<url>images/cuber/vlaunch/9-potential_customers.jpg</url>
				<description>
					<heading>Customers</heading>
					<paragraph>validate your enterprise ...</paragraph>
				</description>
			</slide>

		</slides>
</cu3er>

